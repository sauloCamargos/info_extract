{
  "manifest_version": 3,
  "name": "Starage extract info",
  "version": "0.0.1",
  "permissions": [
    "tabs",
    "unlimitedStorage",
    "storage",
    "activeTab",
    "scripting",
    "declarativeContent"
  ],
  "host_permissions": [
    "http://*/",
    "https://*/"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Storage extract"
  }
}